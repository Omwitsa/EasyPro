        ============================================ -->
    <link rel="stylesheet" href="~/assets/css/meanmenu/meanmenu.min.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/assets/css/animate.css">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/assets/css/normalize.css">
    <!-- mCustomScrollbar CSS
