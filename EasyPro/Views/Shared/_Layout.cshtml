        ============================================ -->
    <script src="~/assets/js/vendor/modernizr-2.8.3.min.js"></script>
</head>
<body>
    <div class="header-top-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="logo-area">
                        <a href="#"><img src="img/logo/logo.png" alt="" /></a>
                    </div>
                </div>
                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                    <div class="header-top-menu">
                        <ul class="nav navbar-nav notika-top-nav">
                            <li class="nav-item dropdown">

                            </li>
                            <li class="nav-item nc-al">

                            </li>
                            <li class="nav-item">

                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Header Top Area -->
    <!-- Mobile Menu start -->
    <div class="mobile-menu-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="mobile-menu">
                        <nav id="dropdown">
                            <ul class="mobile-menu-nav">
                                <li>
                                    <a data-toggle="collapse" data-target="#Charts" href="#">Home</a>
                                    <ul class="collapse dropdown-header-top">
                                        <li><a href="index.html">Dashboard One</a></li>
                                        <li><a href="widgets.html">Widgets</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#demo" href="#">Forms</a>
                                    <ul id="demo" class="collapse dropdown-header-top">
                                        <li><a href="form-elements.html">Form Elements</a></li>
                                        <li><a href="form-components.html">Form Components</a></li>
                                        <li><a href="form-examples.html">Form Examples</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Main Menu area start-->
    <div class="main-menu-area mg-tb-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                        <li>
                            <a data-toggle="tab" href="#file"><i class="notika-icon notika-form"></i>File</a>
                        </li>
                        <li class="active">
                            <a data-toggle="tab" href="#registration"><i class="notika-icon notika-house"></i>Registration</a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#deductions"><i class="notika-icon notika-house"></i>Deductions</a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#activities"><i class="notika-icon notika-form"></i>Activities</a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#setup"><i class="notika-icon notika-form"></i>Setup</a>
                        </li>
                    </ul>
                    <div class="tab-content custom-menu-content">
                        <div id="registration" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a asp-controller="DSuppliers" asp-action="Index">Farmer</a>
                                </li>
                                <li>
                                    <a asp-controller="DTransporters" asp-action="Index">Transporter</a>
                                </li>
                                <li>
                                    <a asp-controller="DDcodes" asp-action="Index">Deduction</a>
                                </li>
                                <li>
                                    <a asp-controller="DBanks" asp-action="Index">Banks</a>
                                </li>
                                <li>
                                    <a asp-controller="DBankBranches" asp-action="Index">Bank Branch</a>
                                </li>

                            </ul>
                        </div>
                        <div id="file" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a asp-controller="SupplierEnquiry" asp-action="Index">Supplier Enquiry</a>
                                </li>
                            </ul>
                        </div>
                        <div id="deductions" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a asp-controller="ProductIntakes" asp-action="DeductionList">Farmer</a>
                                </li>
                                <li>
                                    <a asp-controller="DTransportDeducs" asp-action="Index">Transporter</a>
                                </li>
                                <li>
                                    <a asp-controller="DTransports" asp-action="Index">Transporter Assignement</a>
                                </li>
                            </ul>
                        </div>
                        <div id="activities" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a asp-controller="ProductIntakes" asp-action="Index">Product Intake</a>
                                </li>
                                <li>
                                    <a asp-controller="ProductIntakes" asp-action="CorrectionList">Intake Correction</a>
                                </li>
                                <li>
                                    <a asp-controller="DPayrolls" asp-action="Index">Payroll</a>
                                </li>
                            </ul>
                        </div>
                        <div id="setup" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a asp-controller="DBranchProducts" asp-action="Index">Add Product</a>
                                </li>
                                <li>
                                    <a asp-controller="DPrices" asp-action="Index">Pricing</a>
                                </li>
                                <li>
                                    <a asp-controller="DCompanies" asp-action="Index">Company Setup</a>
                                </li>
                                <li>
                                    <a asp-controller="DBranches" asp-action="Index">Company Branch</a>
                                </li>
                                <li>
                                    <a asp-controller="UserAccounts" asp-action="Index">Users</a>
                                </li>
                                <li>
                                    <a asp-controller="Usergroups" asp-action="Index">User Groups</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Main Menu area End-->

    <div class="normal-table-area">
        <div class="container">
            @RenderBody()
        </div>
    </div>

    <div class="footer-copyright-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="footer-copy-right">
                        <p>
                            Copyright © 2022
                            . All rights reserved by <a href="https://amtechafrica.com/">Amtech Softwares</a>.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- End Footer area-->
    <!-- jquery
        ============================================ -->
    <script src="~/assets/js/vendor/jquery-1.12.4.min.js"></script>
