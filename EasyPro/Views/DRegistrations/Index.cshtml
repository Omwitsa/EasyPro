                @Html.DisplayFor(modelItem => item.Auditid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Auditdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Toledgers)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Datepostedtoledger)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Userledger)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocalId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Run)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
