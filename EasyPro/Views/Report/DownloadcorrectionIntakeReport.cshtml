@model EasyPro.ViewModels.FilterVm

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <form asp-controller="Report" asp-antiforgery="true" asp-action="correctionIntake" method="post">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <label asp-for="Branch" class="control-label"> Branch Milk Correction</label>
                            <select id="ProductType" asp-for="Branch" class="form-control" asp-items="@ViewBag.brances">
                                <option value=""></option>
                            </select>
                            <span asp-validation-for="Branch" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12">
                        <div class="form-group">
                            <label asp-for="DateFrom" class="control-label"></label>
                            <input required asp-for="DateFrom" class="form-control" type="date" />
                            <span asp-validation-for="DateFrom" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12">
                        <div class="form-group">
                            <label asp-for="DateTo" class="control-label"></label>
                            <input required asp-for="DateTo" class="form-control" type="date" />
                            <span asp-validation-for="DateTo" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <button type="submit" class="btn btn-success">Export to Excel</button>
            </form>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Suppliers Intake Report</h3>
                </div>

                <div class="card-body register-card-body">
                    <div class="form-row">
                        <form asp-controller="Report" asp-antiforgery="true" asp-action="Intake" method="post">
                            <div class="row">
                                <div class="col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="DateFrom" class="control-label"></label>
                                        <input id="supplier_intake_from" asp-for="DateFrom" class="form-control" type="date" />
                                        <span asp-validation-for="DateFrom" class="text-danger"></span>
                                    </div>

                                    <button type="submit" class="btn btn-success">Export to Excel</button>
                                </div>

                                <div class="col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="DateTo" class="control-label"></label>
                                        <input id="supplier_intake_to" asp-for="DateTo" class="form-control" type="date" />
                                        <span asp-validation-for="DateTo" class="text-danger"></span>
                                    </div>

                                    <a id="supplier_intake_pdf" class="btn btn-info">Export to PDF</a>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />
    <hr />
    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Branch Suppliers Intake Report</h3>
                </div>
                <div class="card-body register-card-body">
                    <div class="form-row">
                        <form asp-controller="Report" asp-antiforgery="true" asp-action="BranchIntake" method="post">
                            <div class="row">
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="Branch" class="control-label"></label>
                                        <select id="branch" asp-for="Branch" class="form-control" asp-items="@ViewBag.brances">
                                            <option value=""></option>
                                        </select>
                                        <span asp-validation-for="Branch" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="DateFrom" class="control-label"></label>
                                        <input id="branch_sup_intake_from" asp-for="DateFrom" class="form-control" type="date" />
                                        <span asp-validation-for="DateFrom" class="text-danger"></span>
                                    </div>

                                    <button type="submit" class="btn btn-success">Export to Excel</button>
                                </div>

                                <div class="col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="DateTo" class="control-label"></label>
                                        <input id="branch_sup_intake_to" asp-for="DateTo" class="form-control" type="date" />
                                        <span asp-validation-for="DateTo" class="text-danger"></span>
                                    </div>

                                    <a id="branch_sup_intake_pdf" class="btn btn-info">Export to PDF</a>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Summary Suppliers Intake Report</h3>
                </div>
                <form autocomplete="off" asp-controller="Report" asp-antiforgery="true" asp-action="DSumarryIntake" method="post">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group">
                                <label asp-for="Branch" class="control-label"></label>
                                <select id="ProductType" asp-for="Branch" class="form-control" asp-items="@ViewBag.brances">
                                    <option value="" disabled selected hidden>Please Choose...</option>
                                </select>
                                <span asp-validation-for="Branch" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-5 col-md-5 col-sm-5 col-xs-5">
                            <div class="form-group">
                                <label asp-for="DateTo" class="control-label"></label>
                                <input required asp-for="DateTo" class="form-control" type="date" />
                                <span asp-validation-for="DateTo" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class="card-body register-card-body">
                        <div class="form-row">
                            <button type="submit" id="btnsubmit" class="btn btn-success">Export to Excel</button>
                        </div>
                    </div>
                    <div class="card-footer">
                    </div>
                </form>
            </div>
        </div>
    </div>
    <br />
    <hr />
    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Intake Audit Report</h3>
                </div>
                <div class="card-body register-card-body">
                    <div class="form-row">
                        <form asp-controller="Report" asp-antiforgery="true" asp-action="BranchIntakeAudit" method="post">
                            <div class="row">
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="Branch" class="control-label"></label>
                                        <select id="branch" asp-for="Branch" class="form-control" asp-items="@ViewBag.brances">
                                            <option value=""></option>
                                        </select>
                                        <span asp-validation-for="Branch" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="DateFrom" class="control-label"></label>
                                        <input id="branch_sup_intake_from" asp-for="DateFrom" class="form-control" type="date" />
                                        <span asp-validation-for="DateFrom" class="text-danger"></span>
                                    </div>

                                    <button type="submit" class="btn btn-success">Export to Excel</button>
                                </div>

                                <div class="col-sm-6 col-xs-12">
                                    <div class="form-group">
                                        <label asp-for="DateTo" class="control-label"></label>
                                        <input id="branch_sup_intake_to" asp-for="DateTo" class="form-control" type="date" />
                                        <span asp-validation-for="DateTo" class="text-danger"></span>
                                    </div>

                                    <a id="branch_sup_intake_pdf" class="btn btn-info">Export to PDF</a>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

  