@model IEnumerable<EasyPro.Models.DSupplier>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="normal-table-list mg-t-30">
            <div class="row">
                <div class="col-sm-8 col-xs-12">
                    <h3 class="text-danger fa-bold">@ViewBag.sacco Suppliers</h3>
                </div>
            </div>
            <div class="bsc-tbl-hvr table-border-style">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>
                                    Supplier No.
                                </th>
                                <th>
                                    Reg date
                                </th>
                                <th>
                                    Names
                                </th>
                                <th>
                                    Id No.
                                </th>
                                <th>
                                    Account No.
                                </th>
                                <th>
                                    Location
                                </th>
                                <th>
                                    Branch
                                </th>
                                <th>
                                    Phone No.
                                </th>
                                <th>
                                    Active
                                </th>
                            </tr>
                        </thead>
                        <tbody id="geeks">
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Sno)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Regdate)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Names)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.IdNo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.AccNo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Location)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Branch)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PhoneNo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Active)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

